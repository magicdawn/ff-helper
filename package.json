{
  "name": "ff-helper",
  "version": "0.0.1",
  "description": "ffmpeg helper by native binding, using neon",
  "main": "index.node",
  "scripts": {
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "npm run build-release",
    "test": "cargo test"
  },
  "author": "magicdawn",
  "license": "MIT",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/ff-helper.git"
  },
  "keywords": [
    "ffmpeg",
    "ffprobe",
    "neon"
  ],
  "bugs": {
    "url": "https://github.com/magicdawn/ff-helper/issues"
  },
  "homepage": "https://github.com/magicdawn/ff-helper#readme"
}